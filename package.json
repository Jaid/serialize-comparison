{
    "name": "serialize-comparison",
    "main": "./build/index.js",
    "bin": "./build/cli.js",
    "version": "1.0.2",
    "description": "Comparison tool for analyzing performance and efficiency of serialization methods",
    "license": "MIT",
    "author": {
        "name": "Jaid",
        "url": "https://github.com/Jaid",
        "email": "jaid.jsx@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Jaid/serialize-comparison"
    },
    "engines": {
        "node": ">=6.10"
    },
    "files": [
        "build/*"
    ],
    "babel": {
        "extends": "./config/babel.js"
    },
    "eslintConfig": {
        "extends": "./config/eslint.js"
    },
    "keywords": [
        "compressing",
        "compressed",
        "compress",
        "serialize",
        "serializer",
        "serialization",
        "stringify",
        "json",
        "yaml",
        "test",
        "demo",
        "deflate",
        "zlib",
        "benchmark",
        "comparison"
    ],
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.47",
        "@babel/core": "^7.0.0-beta.47",
        "@babel/preset-env": "7.0.0-beta.47",
        "@babel/preset-stage-1": "7.0.0-beta.47",
        "@babel/register": "^7.0.0-beta.47",
        "babel-eslint": "^8.2.3",
        "babel-plugin-lodash": "^3.3.2",
        "babel-preset-minify": "0.4",
        "chalk": "^2.4.1",
        "dist-size": "^0.4.0",
        "ejs": "^2.6.1",
        "eslint": "^4.19.1",
        "faster.js": "^1.1.0",
        "globby": "^8.0.1",
        "html-minifier": "^3.5.15",
        "json5": "^1.0.1",
        "mkdirp": "^0.5.1"
    },
    "dependencies": {
        "base-x": "^3.0.4",
        "compressjs": "^1.0.3",
        "js-yaml": "^3.11.0",
        "jsonpack": "^1.1.5",
        "lodash": "^4.17.10",
        "lz-string": "^1.4.4",
        "lzjs": "^1.3.0",
        "pako": "^1.0.6",
        "tty-table": "^2.6.3"
    },
    "scripts": {
        "start": "node --require @babel/register src/cli.js",
        "start-letter-frequency": "node --require @babel/register research/letterFrequency.js",
        "build-website": "node --require @babel/register website",
        "build-prod": "npx rimraf build && NODE_ENV=production npx babel --out-dir build src",
        "fix": "npx eslint --fix --ext .jsx,.js src",
        "dist-size": "npm run build-prod && npx dist-size build",
        "env": "printenv"
    }
}